{
  "name": "wellcome/intranet-d8",
  "description": "Composer project for Drupal 8 magic for Wellcome Trust Intranet",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "behat/mink-selenium2-driver": "^1.3",
    "bex/behat-screenshot": "^1.2",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.5",
    "drupal/redis": "^1.1",
    "drush/drush": "9.4.0",
    "enyo/dropzone": "^5.5",
    "makinacorpus/php-lucene": "^1.0.2",
    "mikehaertl/phpwkhtmltopdf": "~2.1.",
    "nodespark/des-connector": "6.x-dev",
    "oomphinc/composer-installers-extender": "^1.1",
    "phpmd/phpmd": "^2.6",
    "phpmetrics/phpmetrics": "^2.3",
    "sunra/php-simple-html-dom-parser": "^1.5.2",
    "tecnickcom/tcpdf": "~6",
    "vlucas/phpdotenv": "^2.4",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "drupal/coder": "^8.2",
    "drupal/devel": "^2.0",
    "overtrue/phplint": "^0.2.4",
    "squizlabs/php_codesniffer": "3.*",
    "webflo/drupal-core-strict": "8.7.1"
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "preferred-install": {
      "*": "auto"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "initial": {
        "sites/default/default.services.yml": "sites/default/services.yml",
        "sites/default/default.settings.php": "sites/default/settings.php"
      },
      "omit-defaults": false
    },
    "enable-patching": true,
    "patches": {
    }
  }
}

